{
  "manifest_version": 3,
  "name": "TinyVerse URL Shortener",
  "version": "1.0",
  "description": "Shrink the digital universe with TinyVerse URL shortener.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon_io/favicon-16x16.png",
      "32": "favicon_io/favicon-32x32.png",
      "192": "favicon_io/android-chrome-192x192.png",
      "512": "favicon_io/android-chrome-512x512.png"
    }
  },
  "icons": {
    "16": "favicon_io/favicon-16x16.png",
    "32": "favicon_io/favicon-32x32.png",
    "192": "favicon_io/android-chrome-192x192.png",
    "512": "favicon_io/android-chrome-512x512.png"
  },
  "permissions": [
    "clipboardWrite"
  ]
}